{
  "name": "remcura",
  "version": "1.0.0",
  "description": "Remcura - Professional medical supply inventory management system",
  "private": true,
  "scripts": {
    "i18n:ci": "npm run -s i18n:validate || echo 'no i18n:validate script, skipping'",
    "i18n:postbuild:fail-on-missing": "bash -lc \"if grep -R '\\[MISSING\\]' dist -n; then echo 'i18n missing strings detected in build'; exit 1; else echo 'i18n check passed'; fi\"",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:analyze": "quasar build && npx vite-bundle-analyzer dist/spa",
    "build:stats": "quasar build && echo 'Bundle analysis complete - check dist/spa for generated files'",
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "lint:fix": "eslint \"src/**/*.{ts,vue}\" --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "quality": "npm run lint && npm run stylelint",
    "quality:fix": "npm run format && npm run lint:fix && npm run stylelint:fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue}\" --fix",
    "guard:hardcoded-controls": "node scripts/find-hardcoded-controls.mjs",
    "typecheck": "vue-tsc --noEmit",
    "typecheck:partial": "vue-tsc --noEmit -p tsconfig.partial.json",
    "ci": "npm run quality && npm run typecheck && npm run quality:i18n && npm run build",
    "serve": "quasar serve dist/spa --port 3000 --history",
    "performance": "npm run build && npm run serve",
    "reset-demo": "node scripts/reset-demo-data.js",
    "demo:reset": "node scripts/reset-demo-data.js",
    "translations:status": "node scripts/remcura-translation-manager.js --validate",
    "translations:critical": "node scripts/remcura-translation-manager.js --critical",
    "translations:complete": "node scripts/remcura-translation-manager.js --complete",
    "translations:help": "node scripts/remcura-translation-manager.js --help",
    "i18n:validate": "node scripts/validate-translations.js",
    "i18n:find-hardcoded": "node scripts/find-hardcoded-text.js",
    "i18n:fix-hardcoded-dry": "node scripts/find-hardcoded-text.js --dry-run",
    "i18n:fix-hardcoded": "node scripts/find-hardcoded-text.js --fix",
    "i18n:check": "npm run translations:status && npm run i18n:find-hardcoded",
    "i18n:sync": "node scripts/remcura-translation-manager.js --sync",
    "quality:i18n": "npm run translations:status && npm run i18n:find-hardcoded",
    "prepare": "husky install",
    "guard:dialogs": "node scripts/guard-no-raw-qdialog.js",
    "gs1:import": "ts-node scripts/importGs1Excel.ts",
    "gs1:test": "ts-node scripts/testGs1Import.ts",
    "gs1:help": "ts-node scripts/importGs1Excel.ts --help"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@sentry/tracing": "^7.120.3",
    "@sentry/vue": "^9.33.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/uuid": "^10.0.0",
    "chart.js": "^4.5.0",
    "dotenv": "^16.6.1",
    "glob": "^11.0.3",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "vue": "^3.5.17",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@quasar/app-vite": "^2.2.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.33.0",
    "eslint-webpack-plugin": "^5.0.2",
    "globby": "^14.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-import": "^16.1.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.0",
    "stylelint": "^16.23.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite-bundle-analyzer": "^1.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^3.0.6",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
